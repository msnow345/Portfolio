@import "compass";
// This is compiled with less.php
@import "_base.scss"; // contains reset and common variables 
@import "_vendor.scss"; // contains all vendor prefixes for CSS3 so you don't have to.
@import "_mixins.scss"; // commonly used mixins
@import "_reset.scss"; // reset everything
@import "_typography.scss";
@import "_1k-fixed-grid.scss"; // choose from one of the four grids in the sass folder: 1k grid, 1k fluid, semantic fixed & semantic fluid 
@import "_forms.scss"; // commosnly used typographic page elements - carried throughout all media
@import "_common-elements.scss"; // commonly used typographic page elements - carried throughout all media
@import "_listings.scss";

 

/*==========================================================================
	Layout
==========================================================================*/
html{
	background-color:$color-base-grey;
}

/*==========================================================================
	Headings
==========================================================================*/
.top-header-wrapper{
	width:100%;
	margin:0 auto;
	padding-top:130px;
	text-align:center;

	.heading-normal{
		font-size:65px;
	}

	.heading-stylised{
		font-size:90px;
		position:relative;
	}
}

.heading-normal{
	font-family:$font-bebas;
}

.heading-stylised{
	font-family:$font-lobster-bold;
	word-spacing:8px;
	padding-left:10px;
}

.second-header-wrapper{
	width:100%;
	margin:0 auto;
	padding-top:80px;
	padding-bottom:200px;
	text-align:center;
	opacity:0;
	-webkit-transition: all 0.3s ease-in-out;

	.heading-normal{
		font-size:45px;
	}

	.heading-stylised{
		font-size:55px;
	}
}

/*==========================================================================
	Navigation
==========================================================================*/

.main-nav{
	width:110px;
	height:110px;
	position:absolute;
	top:0;
	left:0;
	cursor:pointer;
	-webkit-perspective: 1200px;
	text-align:center;
	background-color:$color-red;
}

.menu-bar{
	height:11px;
	width:50px;
	margin:0 auto;
	background-color:$color-base-grey;
	margin-bottom:15px;
}

.main-nav-face{
	
	position:absolute;
	top:0;
	left:0;
	width:110px;
	height:110px;
	@include box-model();
	padding-top:23px;
	color:white;
	-webkit-transition: all 0.3s ease-in-out;
}

.main-nav-face-1-wrapper{
	background-color:$color-red;
	position:absolute;
	top:0;
	width:220px;	
	left:0px;
	z-index:8500;
	-webkit-transition: all 0.3s ease-in-out;
}

.main-nav-face-4-wrapper{
	position:absolute;
	top:0;
	width:110px;	
	left:0px;
	z-index:9999;
	-webkit-transition: all 0.5s;
}

.main-nav-face-1-wrapper.three-D{
	width:550px;
}

.main-nav-face-1-wrapper.three-D .main-nav-face-1{
	width:550px;
}

.main-nav-face-4-wrapper.three-D{

	.main-nav-face-4{
	-webkit-transform: rotateY(180deg);
	-webkit-backface-visibility:hidden;
	}

	.main-nav-backface{
	-webkit-transform: rotateY(360deg);
	}

}

.main-nav-face-1{
	background-color:$color-red;
	z-index:8500;
}


.main-nav-face-4{
	-webkit-backface-visibility:hidden;
}


.main-nav-backface{
	-webkit-transform: rotateY(180deg);
	-webkit-backface-visibility:hidden;
	text-align:center;
	font-size:90px;
	font-family:$font-bebas;
	padding-top:5px;
}

.main-nav .standfirst{
	margin-top:113px;
	font-family:$font-bebas;
}

/*==========================================================================
	Sprites
==========================================================================*/

.scroll-down-wrapper{
	width:100%;
	height:100px;
}

.scroll-down{
	background:url('../images/scroll-down.png');
	display:block;
	width:93px;
	height:93px;
	margin:0 auto;
	top:40px;
	font-family:$font-bebas;
	text-align:center;
	padding-top:23px;
	@include box-model();
	opacity:0;
	-webkit-transition: all 0.5s;
	position:relative;
}

/*==========================================================================
	Timelapse
==========================================================================*/

.video-header-wrapper{
	width:100%;
	position:relative;
	max-height:407px;
	overflow:hidden;
	background-color:#1e1e1e;
}

.timelapse{
	width:100%;
	height:auto;
	position:relative;
	-webkit-filter: grayscale(100%);
	margin-top:-10%;
	min-width:812px;
}

.colour-overlay{
	margin-top:-37px;
	position:absolute;
	width:100%;
	top:0;
	background:rgba(167,24,18,0.8);
	max-height:409px;
	top:35px;

	text-align:center;

	.second-heading{
		color:white;
		padding-top:80px;
	}

	.timelapseheight{
		opacity:0;
		width:100%;
		height:auto;
		margin-top:-10%;
		min-width:812px;
	}

	.heading-normal{
		font-size:30px;
	}

	.heading-stylised{
		font-size:40px;
		position:relative;
	}
}

/*==========================================================================
	Accessibility
==========================================================================*/

.nav-skip { position: absolute; overflow: hidden; top:0; left: 0; width: 200px; height: 20px; 
	a { display: block; color: $color-text; position: absolute; left:-300px;top: 0;}
	a:focus, a:active { left: 0; outline: 0; }
}

.accessiblity-features { float:right; padding:14px 0 0; }
.accessiblity-features li { background-image:none; }
.accessiblity-features p.visuallyhidden {display:none;}

.text-size {float:left; margin:0px;font-size:16px;line-height:20px;}
.text-size li {display:inline; padding:2px; font-size:16px;line-height:20px;}
.text-size .small {font-size:16px;}
.text-size .medium {font-size:19px;}
.text-size .large {font-size:22px;}

/*==========================================================================
 breadcrumb 
==========================================================================*/
.breadcrumb{margin-bottom:$spacing;}
.breadcrumb a{@include inline-block(); text-transform:capitalize;}
.breadcrumb span{color:$color-text;padding-right:$spacing/6}
.breadcrumb span.active{color:$color-heading; font-weight:800;text-transform:capitalize;}
.breadcrumb i.chevron{color:$color-text;font-weight:800;padding:0 $spacing/6;}

/*==========================================================================
	Content 
==========================================================================*/

.container {width:978px;margin:0 auto;}

/*==========================================================================
	Footer
==========================================================================*/

.footer {width:978px;margin:0 auto;}

/*==========================================================================
	Animations
==========================================================================*/

@-webkit-keyframes bounce {
  0%   { top:40px; }
  100% { top:35px; }
}

.fade-in{
	opacity:1;
}

.bounce{
	-webkit-animation: bounce 0.5s alternate infinite;
}

.flag-container{
	position:relative;

	.icon{
		width:86px;
		height:86px;
		top:5px;
		background-repeat:no-repeat;
		background-position:top left;
		display:block;
		position:absolute;
		background-image:url('../images/flag.png');
		-webkit-transition: all 0.5s;
		-webkit-transform: rotate(-91deg);
		opacity:0;
	}

	&.active {
		.icon{
		-webkit-transform: rotate(-10deg);
		opacity:1;
		}
	}
}

/*==========================================================================
	Non Semantic Classes
==========================================================================*/

.ir { display: block; text-indent: -999em; overflow: hidden; background-repeat: no-repeat; text-align: left; direction: ltr; }
.hidden { display: none; visibility: hidden;}
.hide { display: none;}
.visuallyhidden { border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px; }
.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus { clip: auto; height: auto; margin: 0; overflow: visible; position: static; width: auto; }
.invisible { visibility: hidden; }
.clearfix{@include clearfix();}
.alignleft {float:left;}
.alignright {float:right;}
.clear {clear:both;}
.printonly {display:none;}
